<Application x:Class="EzHRMApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EzHRMApp"
             StartupUri="Windows/LoginWindow.xaml"
             Exit="Application_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/CornControls;component/Style/ScrollBar.xaml"/>
                <ResourceDictionary Source="/CornControls;component/Themes/ColorScheme.xaml"/>
                <ResourceDictionary Source="/CornControls;component/CustomControl/Converters/ConverterDict.xaml"/>
                <ResourceDictionary Source="Themes/Geometry.xaml"/>
                <ResourceDictionary Source="Icons/IconsDictionary.xaml"/>
                <ResourceDictionary Source="Core/ControlsStyle/Controls.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
